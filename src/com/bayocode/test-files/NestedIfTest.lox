assign num = 0;
input "Enter a number: " num;

output "Checking your number: ";
output num;

if (num > 0) {
    output "Your number is positive.";
    
    if (num > 10) {
        output "Your number is greater than 10.";
        
        if (num > 100) {
            output "Your number is greater than 100.";
        } else {
            output "Your number is between 11 and 100.";
        }
    } else {
        output "Your number is between 1 and 10.";
    }
} else if (num < 0) {
    output "Your number is negative.";
    
    if (num < -10) {
        output "Your number is less than -10.";
    } else {
        output "Your number is between -10 and -1.";
    }
} else {
    output "Your number is zero.";
}

output "Analysis complete!";
